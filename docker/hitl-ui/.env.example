# aSDLC HITL UI Environment Configuration
# Copy this file to .env.local for local development

# API base URL (in development, Vite proxy is used)
VITE_API_BASE_URL=/api

# WebSocket URL for real-time event streaming
VITE_WS_URL=ws://orchestrator:8080/ws

# Multi-tenancy settings
VITE_MULTI_TENANCY_ENABLED=false
VITE_ALLOWED_TENANTS=default

# Polling interval in milliseconds
VITE_POLLING_INTERVAL=10000

# Use mock data (for development without backend)
VITE_USE_MOCKS=true
